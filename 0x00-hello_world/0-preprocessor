#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**
* main - random number, could be +, - or zero
* include <studio.h, libreria de printf
* srand - pseudo random number that is generated with seed
* print f: prints a message
* Return: 0
*/
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
 	if (n > 0)
	{
		printf("%n is positive\n", n);
	}
	else if (n == 0)
	{
		printf("%n is zero\n", n);
	}
	else
		printf("%n is negative\n", n);
	}
	return (0);
}
